#!/bin/bash

DIR="$(dirname "$0")"
. "${DIR}/config"

set -o errexit
set -o pipefail

gcloud pubsub subscriptions delete gcr-sub -q
gcloud pubsub topics delete gcr -q

gcloud run services delete $ROOT_NAME --region $REGION -q

gcloud artifacts repositories delete $REGISTRY_NAME --location $REGION -q

gcloud iam service-accounts delete $SA_EMAIL -q
